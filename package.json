{
  "name": "vue3-ts-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint-prettier": "prettier --write ./**/*.{js,json,wxss,wxml,wxs,vue,ts}",
    "prepare": "husky install",
    "es-lint": "eslint --fix"
